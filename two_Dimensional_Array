#include<stdio.h>
//array com 2 linhas e 3 colunas
//inserir numero na posicao
//insira numero para posicao tal
//mostrar o array
#include<stdio.h>

int main() {
    int numero[2][3];

    // preencher o vetor
    for(int i = 0; i < 2; i++) {
        for(int j = 0; j < 3; j++) {
            printf("Inserir numero na posicao [%d][%d]: ", i, j);
            scanf("%d", &numero[i][j]);
        }
    }

    // exibir o vetor preenchido
    printf("Array:\n");
    for(int i = 0; i < 2; i++) {
        for(int j = 0; j < 3; j++) {
            printf("%d  ", numero[i][j]);
        }
        printf("\n");
    }

    return 0;
}
